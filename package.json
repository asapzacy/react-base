{
  "name": "react-base",
  "description": "minimal react project -- base files",
  "author": "Zac Arellano",
  "license": "MIT",
  "version": "1.0.0",
  "private": true,
  "main": "app.js",
  "engines": {
    "node": ">= 7.6.0"
  },
  "scripts": {
    "dev": "env UV_THREADPOOL_SIZE=100 webpack-dev-server --progress --colors",
    "server": "nodemon app.js",
    "start": "concurrently 'yarn dev' 'yarn server' --names 'REACT,NODE' -c 'bgBlue.bold,bgMagenta.bold'",
    "start:fresh": "yarn reset:all && yarn start",
    "build": "yarn && NODE_ENV='production' webpack --progress --colors",
    "build:deploy": "yarn reset:all && yarn build",
    "reset:cache": "rm -rf dist/assets/build/* && rm -f dist/index.html",
    "reset:npm": "rm -rf node_modules && yarn",
    "reset:all": "yarn reset:npm && yarn reset:cache",
    "lint": "eslint src/.; exit 0",
    "lint:fix": "eslint --fix src/.; exit 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "svg": "find dist/assets -name '*.svg' -type f -exec yarn svgo '{}' \\;",
    "svgo": "svgo --config='{ \"plugins\": [{ \"removeTitle\": false },{ \"removeViewBox\": false }]}'"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.35",
    "@babel/core": "^7.0.0-beta.35",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.35",
    "@babel/plugin-syntax-decorators": "^7.0.0-beta.35",
    "@babel/plugin-transform-arrow-functions": "^7.0.0-beta.35",
    "@babel/preset-env": "^7.0.0-beta.35",
    "@babel/preset-react": "^7.0.0-beta.35",
    "@babel/preset-stage-0": "^7.0.0-beta.35",
    "babel-eslint": "^8.0.3",
    "babel-loader": "8.0.0-beta.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "nodemon": "^1.13.3",
    "postcss-assets-webpack-plugin": "^1.1.0",
    "postcss-loader": "^2.0.9",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "style-loader": "^0.19.1",
    "svgo": "^1.0.3",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asapzacy/react-base.git"
  },
  "bugs": {
    "url": "https://github.com/asapzacy/react-base/issues"
  }
}
